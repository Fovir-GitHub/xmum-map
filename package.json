    "wdev": "next dev",
    "wbuild": "pnpm run prebuild && next build",
    "test:sitemap": "node src/scripts/testSitemap.js"
  },
  "type": "module",
  "dependencies": {
    "@catppuccin/palette": "1.7.1",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.3.1",
    "@mui/material": "7.3.1",
    "next": "15.5.0",
    "next-sitemap": "4.2.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-zoom-pan-pinch": "3.7.0",
    "remark": "15.0.1",
    "remark-gfm": "4.0.1",
    "remark-html": "16.0.1",
    "swiper": "11.2.10",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "prettier": "3.6.2"
  }
}
