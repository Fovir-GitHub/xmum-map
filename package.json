{
  "name": "XMUM Map",
  "version": "0.3.2",
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "pre:data": "node src/scripts/readSvgData.js",
    "prebuild": "pnpm pre:data",
    "build": "NODE_ENV=production next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "start": "next start",
